<!doctype html>
<html lang="en">
  <!-- If you're reading this: help us obi-wan can-coder, you're our only hope. Apply today: ogs.ny.gov/employment -->
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="{{title}}">
    <meta name="generator" content="11ty/eleventy">    
    <!-- Facebook Meta Tags -->
    <meta property="og:url" content="https://{{url}}">
    <meta property="og:type" content="website">
    <meta property="og:title" content="{{facebook.title}}">
    <meta property="og:description" content="{{facebook.description}}">
    <meta property="og:image" content="https://{{domain}}/img/{{facebook.image}}">
    <!-- Twitter Meta Tags -->
    <meta name="twitter:card" content="summary_large_image">
    <meta property="twitter:url" content="https://{{url}}">
    <meta name="twitter:title" content="{{twitter.title}}">
    <meta name="twitter:description" content="{{twitter.description}}">
    <meta name="twitter:image" content="https://{{domain}}/img/{{twitter.image}}">
    {% include '_css-link.njk' %}
    {% include '_js-link.njk' %}
    <title>{{ title }}</title>
  </head>
  <body class="nysds-normalize">
  <a href="#main-site-content-for-skiplink" class="sr-only focus:not-sr-only text-blue-700 underline">Skip to Main Content</a>
    {% from '../_includes/_components.njk' import unavcurrent,unavcurrentfooter,nav,footer  %}
      {{unavcurrent({})}}
     {% include 'nav-data.njk' %}
    <main class="mx-8 md:mx-20" id="main-site-content-for-skiplink">
    {{ content | safe }}
    </main>
    {% include 'footer-data.njk' %}
    {{unavcurrentfooter({})}}
  </body>
</html>



